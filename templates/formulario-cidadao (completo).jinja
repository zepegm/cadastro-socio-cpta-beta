    <script>
      // Example starter JavaScript for disabling form submissions if there are invalid fields
      (function() {
        'use strict';
        window.addEventListener('load', function() {
          // Fetch all the forms we want to apply custom Bootstrap validation styles to
          var forms = document.getElementsByClassName('needs-validation');
          // Loop over them and prevent submission
          var validation = Array.prototype.filter.call(forms, function(form) {
            form.addEventListener('submit', function(event) {

              if (form.checkValidity() === false) {
                event.preventDefault();
                event.stopPropagation();
              }

              var familiares = []
              $("#familia tbody tr").each(function() {
                var nome = $("th input", this).val();
                var cpf = $("th", this).next().find("input", this).val();
                var estado_civil = $("th", this).next().next().find("select", this).val();
                var vinculo = $("th", this).next().next().next().find("input", this).val();
                var nascimento = $("th", this).next().next().next().next().find("input", this).val();
                var escolaridade = $("th", this).next().next().next().next().next().find("select", this).val();
                var profissao = $("th", this).next().next().next().next().next().next().find("input", this).val();
                if ($("th", this).next().next().next().next().next().next().next().find("input:checked", this).val() == "on") {
                  var formal = true;
                } else {
                  var formal = false;
                }
                var saude = $("th", this).next().next().next().next().next().next().next().next().find("select", this).val();

                var membro = {'nome':nome, 'cpf':cpf, 'estado_civil':estado_civil, 'vinculo':vinculo, 'nascimento':nascimento, 'escolaridade':escolaridade, 'profissao':profissao, 'formal':formal, 'saude':saude};
                familiares.push(membro);
              });

              //console.log(familiares);
              $("#composicao_familiar").val(JSON.stringify(familiares));

              form.classList.add('was-validated');
            }, false);
          });
        }, false);
      })();

